{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "ie >= 11",
                        "safari >= 7"
                    ],
                    "esmodules": true
                },
                "useBuiltIns": "entry"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        ["module-resolver", {
            "root": ["./src"],
            "alias": {
                "dist": "./dist",
                "underscore": "lodash"
            }
        }]
    ]
}